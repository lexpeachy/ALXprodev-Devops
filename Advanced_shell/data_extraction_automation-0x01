#!/bin/bash

# Extract raw data using jq
name=$(jq -r '.name' data.json)
height=$(jq -r '.height / 10' data.json)
weight=$(jq -r '.weight / 10' data.json)
type=$(jq -r '.types[0].type.name' data.json)

# Format output to match the exact test requirement
echo "$name is of type $type, weighs ${weight}kg, and is ${height}m tall."
